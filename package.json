{
	"name": "@tonejs/examples",
	"version": "0.0.1",
	"description": "Tone.js Examples",
	"main": "index.js",
	"scripts": {
		"watch:html": "nodemon --exec \"npm run build:html\" --ext ts,hbs --ignore scripts/type.ts --ignore scripts/sidebar.ts",
		"watch:css": "npm run build:css -- -w",
		"build:html": "node -r ts-node/register ./page/index.ts --output=$npm_config_output",
		"build:css": "node-sass ./page/style/ -o ./assets/style",
		"lint": "eslint --ext ts ./**/*.ts --fix",
		"test": "echo \"Error: no test specified\" && exit 1",
		"watch": "webpack --watch --mode=development"
	},
	"nodemonConfig": {
		"watch": [
			"page"
		]
	},
	"author": "",
	"license": "GPL-3.0",
	"devDependencies": {
		"@types/node": "^12.12.8",
		"@typescript-eslint/eslint-plugin": "^2.7.0",
		"@typescript-eslint/parser": "^2.7.0",
		"css-loader": "^3.2.0",
		"deep-equal": "^1.1.1",
		"eslint": "^6.6.0",
		"eslint-plugin-jsdoc": "^18.0.1",
		"fs-extra": "^8.1.0",
		"glob": "^7.1.6",
		"glob-promise": "^3.4.0",
		"handlebars": "^4.5.3",
		"lit-element": "^2.2.1",
		"marked": "^0.7.0",
		"node-sass": "^4.13.0",
		"nodemon": "^1.19.4",
		"sass-loader": "^8.0.0",
		"style-loader": "^1.0.0",
		"svg-inline-loader": "^0.8.0",
		"to-string-loader": "^1.1.6",
		"ts-loader": "^6.2.1",
		"ts-node": "^8.5.2",
		"typescript": "^3.7.2",
		"webpack": "^4.41.2",
		"webpack-cli": "^3.3.10"
	},
	"dependencies": {
		"tone": "^14.4.78"
	}
}
